loop from 1-10
check for multipes of 3
check for multipes of 5
sum all multiplies
var sum =0;
for(var i=1; i<10; i++){
	if (i % 3 == 0 || i % 5 ==0 ) {
		sum += i;
	}
}
console.log(sum);

